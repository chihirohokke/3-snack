<% if @users.any? %>
  <ul class="list-unstyled">
    <% @users.each do |user| %>
    <li class="media">
      <% if user.image? %>
        <%= link_to image_tag(user.image.url), user_path(user), class: "mr-2 " %>
      <% end %>
      <div>
        <%= link_to user.name, user_path(user) %> 
      </div>
    </li>
    <% end %>
  </ul>
  <%= paginate @users %>
<% end %>  